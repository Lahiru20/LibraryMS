<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="register_frame" prefHeight="361.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="formcontroller.RegisterFormController">
   <children>
      <ImageView fitHeight="386.0" fitWidth="600.0" layoutY="-8.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/admin_signin.png" />
         </image>
      </ImageView>
      <TextField fx:id="txt_username" layoutX="47.0" layoutY="151.0" prefHeight="27.0" prefWidth="208.0" promptText="Username" style="-fx-border-color: #000000; -fx-border-radius: 4px;" />
      <TextField fx:id="txt_Email" layoutX="48.0" layoutY="189.0" prefHeight="26.0" prefWidth="208.0" promptText="Email" style="-fx-border-color: #000000; -fx-border-radius: 4px;" />
      <Button layoutX="48.0" layoutY="307.0" mnemonicParsing="false" onAction="#btn_Action_Action" prefHeight="26.0" prefWidth="208.0" style="-fx-background-color: #000000;" text="REGISTER" textFill="WHITE" />
      <Button layoutX="377.0" layoutY="251.0" mnemonicParsing="false" onAction="#Login_Action" prefHeight="26.0" prefWidth="147.0" style="-fx-background-color: #000000; -fx-border-color: #ffffff; -fx-border-radius: 8px;" text="LOGIN" textFill="#fffcfc" />
      <PasswordField fx:id="txt_Password" layoutX="48.0" layoutY="226.0" prefHeight="26.0" prefWidth="208.0" promptText="Password" style="-fx-border-color: #000000; -fx-border-radius: 4px;" />
      <PasswordField fx:id="txt_conform_Password" layoutX="48.0" layoutY="263.0" prefHeight="26.0" prefWidth="208.0" promptText="Confirm Password" style="-fx-border-color: #000000; -fx-border-radius: 4px;" />
   </children>
</AnchorPane>
